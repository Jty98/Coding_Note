package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 구구단 출력하기
		//int[] dan = {5, 3, 9, 7, 8};
		
		for (int dan = 1; dan <= 9; dan++) {
			for (int num = 2; num <= 9; num++) {
				System.out.print(num + " X " + dan + " = " + (dan * num) + "	");
			}
			System.out.println();
		}
	}

}
