package com.javalec.base;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		
		String score ="";
		int num = 0;
		Scanner scanner = new Scanner(System.in);
		
		System.out.print("점수를 입력하세요 : ");
		num = scanner.nextInt();
		
		switch(num & 90) {
		case 0:
			score = "A";
			break;
		case 1:
			score = "B";
			break;
		default:
			break;
		}
		

	}

}
