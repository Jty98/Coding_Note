package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 구구단 출력하기
		int[] dan = { 5, 3, 9, 7, 8 };
		int sum = 0; 

		for (int num2 = 1; num2 <= 9; num2++) {
			for (int num = 2; num <= 9; num++) {
				sum += dan[num];
				if (sum == num) {
					
					System.out.print(num + " X " + num2 + " = " + (num2 * num) + "	");
				}

			}
			System.out.println();
		}
	}
}
