package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 배열의 평균 구하기
		int[] intNum = {1, 5, 8, 12, 200, 1002};
		int sum = 0;
		double avg = 0;
		
		for(int i=0; i<intNum.length; i++) {
			sum += intNum[i];
			System.out.println(intNum[i]);
			avg = intNum[i]/3;
		}
		System.out.println(avg);
		

	}

}
