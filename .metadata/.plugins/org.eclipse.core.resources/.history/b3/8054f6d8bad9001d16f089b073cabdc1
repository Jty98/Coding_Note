package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 구구단 출력하기
		String[] str = { "Sun", "Mon", "Tue", "Wed", "Thr", "Fri", "Set" };
		int[] inNum = { 10, 20, 30 };
		double[] doubleNum = { 1.1, 1.2 };
		boolean[] boolNum = { true, false, true, true };

		int sum = 0; // str출력변수
		int sum1 = 0; // intNum출력변수
		double avg = 0; // doubleNum평균변수

		// str 전체 출력해보기
		for (int i = 0; i < str.length; i++) {
			sum += i;
			System.out.print(str[i] + "/t");
		}
		// intNum의 합계 구하기
		sum = 0;
		for (int i = 0; i < inNum.length; i++) {
			sum += inNum[i];
		}
		System.out.println("intNum의 합계 : " + sum);

		// doubleNum의 평균 구하기
		sum = 0;
		for (int i = 0; i < doubleNum.length; i++) {
			sum += doubleNum[i];
			avg = sum / doubleNum.length;
		}
		System.out.println("doubleNum의 평균 : " + avg);

		// boolNum중 true의 갯수 구하기
		sum = 0;
		for (int i = 0; i < boolNum.length; i++) {
			if(boolNum[i] == true) {
				sum += 1;
			}
		}
		System.out.println("true의 갯수 : " + sum);
		}

		
	
}
