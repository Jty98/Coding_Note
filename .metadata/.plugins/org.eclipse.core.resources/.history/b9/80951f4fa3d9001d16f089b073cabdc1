package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 배열(Array)
		// 문자열 배열 데이터 7개를 생성
		String[] str = new String[7]; // 메모리에 새로 저장할때는 new
		str[0] = "일요일";
		str[1] = "월요일";
		str[2] = "화요일";
		str[3] = "수요일";
		str[4] = "목요일";
		str[5] = "금요일";
		str[6] = "토요일";
		
		// 배열의 갯수 확인하기
		System.out.println(str.length);
		
		for(int i=0; i<str.length; i++) {
			System.out.println(str[i]);
		}

	}

}
